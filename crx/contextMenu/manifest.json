{
  "manifest_version":2,
  "name": "右键菜单Demo",
  "version": "1.0",
  "description": "右键菜单Demo",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "images/icon16.png" 
  },
  "content_scripts": [
    {
        "matches": ["*://*/*"],
        "js": ["js/content.js"]
    }
  ],  
  "background": {
    "scripts": [
        "js/background.js"
    ]
  }

}